{
    "name": "krbotman/driver-facebook",
    "license": "MIT",
    "description": "Facebook Messenger driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Facebook Messenger",
        "Facebook",
        "Messenger"
    ],
    "homepage": "http://github.com/botman/driver-facebook",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "krbotman/botman": "~3.0"
    },
    "require-dev": {
        "krbotman/studio-addons": "1.6.*",
        "phpunit/phpunit": "^7.0|^8.0",
        "illuminate/contracts": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "mockery/mockery": "dev-master",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Facebook\\Providers\\FacebookServiceProvider"
            ]
        }
    }
}
